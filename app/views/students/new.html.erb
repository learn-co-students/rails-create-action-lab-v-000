<h2>Student Form</h2>

<%= form_tag students_path do %>
  <%= label_tag 'first_name' %>
  <%= text_field_tag 'first_name' %><br><br>
  <%= label_tag 'last_name' %>
  <%= text_field_tag 'last_name' %><br><br>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

  <%= submit_tag 'Submit Student' %>
<% end %>
