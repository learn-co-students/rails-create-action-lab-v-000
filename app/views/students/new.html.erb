<h3>Student Form</h3>

<%= form_tag students_path do %>

  <label>First Name:</label>
    <input type="text" name="first_name" placeholder="First Name:">

  <label>Last Name:</label>
      <input type="text" name="last_name" placeholder="Last Name:">

<%= submit_tag "Submit Student" %>
<% end %>
