<h3>Student Form</h3>
<%= form_tag students_path do %>
  <fieldset>
    <label for="first_name">First Name:</label>
    <%= text_field_tag :first_name %>
  </fieldset>
  <fieldset>
    <label for="last_name">Last Name:</label>
    <%= text_field_tag :last_name %>
  </fieldset>
  <%= submit_tag "Submit Student" %>
<% end %>
